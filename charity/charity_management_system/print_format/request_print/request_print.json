 "html": "{% set client = frappe.get_doc(\"Client\", doc.file_number)%}\n{% set coupon = frappe.get_list(\"Coupon\",filters={\"request_number\": doc.request_number},fields=[\"*\"])%}\n{% set cheque_specification = frappe.get_list(\"Cheque Specification\",filters={\"request_number\": doc.request_number},fields=[\"*\"])%}\n<div class=\"RQ\">\n<table width=\"100%\">\n    <tr>\n      <td style=\"width: 20%\" rowspan=\"3\">{{ client.image or \"\" }}</td>\n      <td style=\"width: 25%\" rowspan=\"3\"><img src=\"/files/123123.png\" /></td>\n      <td style=\"width: 25%\" align=\"center\">\n      <p>\u062c\u0645\u0639\u064a\u0629 \u0633\u064a\u0647\u0627\u062a \u0644\u0644\u062e\u062f\u0645\u0627\u062a \u0627\u0644\u0625\u062c\u062a\u0645\u0627\u0639\u064a\u0629</p><br>\n          <p>\u0645\u0633\u062c\u0644\u0629 \u0628\u0648\u0632\u0627\u0631\u0629 \u0627\u0644\u0634\u0626\u0648\u0646 \u0627\u0644\u0625\u062c\u062a\u0645\u0627\u0639\u064a\u0629 \u062a\u062d\u062a \u0631\u0642\u0645 6</p><br>\n            <p>\u0641\u0631\u0639 \u0627\u0644\u0645\u062d\u0645\u062f\u064a\u0629 \u0628\u0627\u0644\u062f\u0645\u0627\u0645</p>\n      </td>\n    </tr>\n</table>\n<table border=1 width=\"100%\" class=\"table table-bordered\">\n  <tr>\n    <th colspan=\"8\">:\u0623\u0648\u0644\u0627\u064b : \u0628\u064a\u0627\u0646\u0627\u062a \u0634\u062e\u0635\u064a\u0629 \u0644\u0637\u0627\u0644\u0628 \u0627\u0644\u0645\u0633\u0627\u0639\u062f\u0629</th>\n  </tr>\n  <tr>\n    <td>{{ client.age or \"\"}}</td>\n    <td style=\"font-weight: bold\">\u0627\u0644\u0639\u0645\u0631</td>\n    <td>{{ doc.h_date_of_request or \"\"}}</td>\n    <td style=\"font-weight: bold\">\u062a\u0627\u0631\u064a\u062e \u0627\u0644\u0637\u0644\u0628</td>\n    <td>{{ doc.file_number or \"\"}}</td>\n    <td style=\"font-weight: bold\">\u0631\u0642\u0645 \u0627\u0644\u0645\u0644\u0641</td>\n    <td>{{doc.name or \"\"}}</td>\n    <td style=\"font-weight: bold\">\u0631\u0642\u0645 \u0627\u0644\u062d\u0627\u0644\u0629</td>\n  </tr>\n  <tr>\n    <td>{{client.place_of_issue or \"\"}}</td>\n    <td style=\"font-weight: bold\">\u0645\u0643\u0627\u0646 \u0627\u0644\u0625\u0635\u062f\u0627\u0631</td>\n    <td>{{ doc.national_id or \"\"}}</td>\n    <td style=\"font-weight: bold\">\u0627\u0644\u0647\u0648\u064a\u0629 \u0627\u0644\u0648\u0637\u0646\u064a\u0629</td>\n    <td colspan=\"3\">{{ doc.full_name or \"\"}}</td>\n    <td style=\"font-weight: bold\">\u0627\u0644\u0627\u0633\u0645 \u0627\u0644\u0631\u0628\u0627\u0639\u064a</td>\n  </tr>\n  <tr>\n    <td>{{client.joining_type or \"\"}}</td>\n    <td style=\"font-weight: bold\">\u0646\u0648\u0639 \u0627\u0644\u0625\u0646\u0636\u0645\u0627\u0645</td>\n    <td>{{client.start_date or \"\"}}</td>\n    <td style=\"font-weight: bold\">\u0628\u062f\u0627\u064a\u0629 \u0627\u0644\u0625\u0646\u0636\u0645\u0627\u0645</td>\n    <td>{{client.place_of_birth or \"\"}}</td>\n    <td style=\"font-weight: bold\">\u0645\u0643\u0627\u0646 \u0627\u0644\u0645\u064a\u0644\u0627\u062f</td>\n    <td>{{client.date_of_birth or \"\"}}</td>\n    <td style=\"font-weight: bold\">\u062a\u0627\u0631\u064a\u062e \u0627\u0644\u0645\u064a\u0644\u0627\u062f</td>\n  </tr>\n  <tr>\n    <td>{{client.the_joining_period or \"\"}}</td>\n    <td style=\"font-weight: bold\">\u0645\u062f\u0629 \u0627\u0644\u0625\u0646\u0636\u0645\u0627\u0645</td>\n    <td>{{client.work_telephone_no or \"\"}}</td>\n    <td style=\"font-weight: bold\">\u0647\u0627\u062a\u0641 \u0627\u0644\u0639\u0645\u0644</td>\n    <td>{{client.work_place or \"\"}}</td>\n    <td style=\"font-weight: bold\">\u0645\u0643\u0627\u0646 \u0627\u0644\u0639\u0645\u0644</td>\n    <td>{{client.job_title or \"\"}}</td>\n    <td style=\"font-weight: bold\">\u0645\u0633\u0645\u0649 \u0627\u0644\u0648\u0638\u064a\u0641\u0629</td>\n  </tr>\n  <tr>\n    <td>{{client.education_status or \"\"}}</td>\n    <td style=\"font-weight: bold\">\u0627\u0644\u0645\u0633\u062a\u0648\u0649 \u0627\u0644\u062a\u0639\u0644\u064a\u0645\u064a</td>\n    <td