 "html": "{% set coupon = frappe.get_list(\"Coupon\",filters={\"request_number\": doc.request_number},fields=[\"*\"])%}\n{%- for row in doc.recommendation_table -%}\n\n{% set u = frappe.get_doc(\"User\", doc.owner) %}\n\n<div>\n  <br>\n  <h3>\u0643\u0648\u0628\u0648\u0646 \u0645\u0634\u062a\u0631\u064a\u0627\u062a</h3>\n  <br>\n  <br>\n  <table width=\"100%\">\n    <tr>\n      <td width=\"15%\">{{ row.date_of_coupon or \"\" }}</td>\n      <td width=\"12%\">\u0627\u0644\u062a\u0627\u0631\u064a\u062e</td>\n      <td width=\"12%\">{{ row.mobile_number or \"\" }}</td>\n      <td width=\"13%\">\u0631\u0642\u0645 \u0627\u0644\u062c\u0648\u0627\u0644</td>\n      <td width=\"12%\">{{ row.request_number or \"\" }}</td>\n      <td width=\"12.5%\">\u0631\u0642\u0645 \u0627\u0644\u062d\u0627\u0644\u0629</td>\n      <td width=\"11.5%\">{{ row.file_number or \"\" }}</td>\n      <td width=\"12%\">\u0631\u0642\u0645 \u0627\u0644\u0645\u0644\u0641</td>\n    </tr>\n    <tr>\n      <td colspan=\"5\">{{ row.description or \"\" }}</td>\n      <td colspan=\"2\">{{ row.the_company or \"\" }}</td>\n      <td>/ \u0627\u0644\u0633\u0627\u062f\u0629</td>\n    </tr>\n    <tr>\n      <td colspan=\"8\">\u0628\u0646\u0627\u0621 \u0639\u0644\u0649 \u0627\u0644\u0625\u062a\u0641\u0627\u0642 \u0645\u0639\u0643\u0645 \u0646\u0623\u0645\u0644 \u0635\u0631\u0641 \u0627\u0644\u062a\u0627\u0644\u064a</td>\n    </tr>\n    <tr>\n      <td colspan=\"1\" height=30px>{{ row.quantity or \"\" }}</td>\n      <td colspan=\"1\" height=30px>\u0627\u0644\u0643\u0645\u064a\u0629</td>\n      <td colspan=\"4\" height=30px>{{ row.item or \"\" }}</td>\n      <td colspan=\"2\" height=30px>{{ row.package_type or \"\" }}</td>\n    </tr>\n    <tr>\n      <td colspan=\"8\">\u0634\u0627\u0645\u0644 \u0627\u0644\u062a\u0648\u0635\u064a\u0644 \u0648\u0627\u0644\u062a\u0631\u0643\u064a\u0628, \u0648\u0627\u0644\u0625\u062d\u062a\u0641\u0627\u0638 \u0628\u0623\u0635\u0644 \u0627\u0644\u0643\u0648\u0628\u0648\u0646 \u0644\u062f\u064a\u0643\u0645 \u0648\u062a\u0633\u0644\u064a\u0645\u0647 \u0644\u0644\u062c\u0645\u0639\u064a\u0629 \u0641\u064a \u0646\u0647\u0627\u064a\u0629 \u0643\u0644 \u0634\u0647\u0631 \u0645\u0631\u0641\u0642\u0627 \u0628\u0627\u0644\u0641\u0627\u062a\u0648\u0631\u0629</td>\n    </tr>\n  </table>\n<br>\n<br>\n  <table width=\"100%\">\n    <tr>\n      <td>\u0625\u062f\u0627\u0631\u0629 \u0627\u0644\u0641\u0631\u0639</td>\n      <td>\u0627\u0644\u062e\u062a\u0645</td>\n      <td>\u062a\u0648\u0642\u064a\u0639 \u0627\u0644\u0645\u0633\u062a\u0644\u0645</td>\n      <td>\u0645\u0639\u062f \u0627\u0644\u0643\u0648\u0628\u0648\u0646</td>\n    </tr>\n    <tr>\n      <td height=40px></td>\n      <td></td>\n      <td></td>\n      <td>{{frappe.session.user}}</td>\n    </tr>\n  </table>\n</div>\n{%- endfor -%}", 
