 "html": "{% set cheque_specification = frappe.get_list(\"Cheque Specification\",filters={\"request_number\": doc.request_number},fields=[\"*\"])%}\n  {%- for row in doc.cheque -%}\n\n<div>\n  <br>\n  <br>\n  <br>\n  <br>\n  <br>\n  <table width=\"100%\">\n    <tr>\n      <td>{{ row.h_date_of_issue or \"\" }}</td>\n      <td>\u0627\u0644\u062a\u0627\u0631\u064a\u062e</td>\n      <td>{{ row.mobile_number or \"\" }}</td>\n      <td>\u0631\u0642\u0645 \u0627\u0644\u062c\u0648\u0627\u0644</td>\n      <td>{{ row.request_number or \"\" }}</td>\n      <td>\u0631\u0642\u0645 \u0627\u0644\u062d\u0627\u0644\u0629</td>\n      <td>{{ row.file_number or \"\" }}</td>\n      <td>\u0631\u0642\u0645 \u0627\u0644\u0645\u0644\u0641</td>\n      <td>{{ row.name_of_client or \"\" }}</td> \n      <td>\u0627\u0644\u0625\u0633\u0645</td>\n    </tr>\n    <tr>\n      <td colspan=\"1\">\u0627\u0644\u0645\u062d\u062a\u0631\u0645</td>\n      <td colspan=\"7\">\u0646\u0627\u0626\u0628 \u0645\u062f\u064a\u0631 \u0641\u0631\u0639 \u062c\u0645\u0639\u064a\u0629 \u0633\u064a\u0647\u0627\u062a \u0628\u0627\u0644\u062f\u0645\u0627\u0645</td>\n      <td colspan=\"2\">/ \u0627\u0644\u0645\u0643\u0631\u0645</td>\n    </tr>\n    <tr>\n      <td colspan=\"4\">\u0631\u064a\u0627\u0644 \u0644\u0644\u0645\u0633\u062a\u0641\u064a\u062f \u0627\u0644\u0645\u0630\u0643\u0648\u0631 \u0628\u064a\u0627\u0646\u0627\u062a\u0647 \u0623\u0639\u0644\u0627\u0647</td>\n      <td colspan=\"2\">{{ row.cheque_amount or \"\" }}</td> \n      <td colspan=\"4\">\u0646\u0623\u0645\u0644 \u0627\u0644\u0645\u0648\u0627\u0641\u0642\u0629 \u0639\u0644\u0649 \u0635\u0631\u0641 \u0645\u0628\u0644\u063a \u0648\u0642\u062f\u0631\u0647</td>\n    </tr>\n    <tr>\n      <td colspan=\"7\" height=30px>{{ row.cheque_owner or \"\" }}</td>\n      <td colspan=\"3\">\u0627\u0644\u0645\u0628\u0644\u063a \u064a\u0635\u0631\u0641 \u0628\u0627\u0633\u0645</td>\n    </tr>\n    <tr>\n      <td colspan=\"10\" class=\"ID\">\u060c\u060c\u060c \u0648\u0644\u0640\u0643\u0640\u0645 \u062c\u0632\u064a\u0644 \u0627\u0644\u0634\u0643\u0631</td>\n    </tr>\n  </table>\n  <table width=\"100%\">\n    <tr>\n      <td colspan=\"2\"></td>\n      <td colspan=\"3\">\u0644\u062c\u0646\u0629 \u0627\u0644\u0628\u062d\u062b \u0627\u0644\u0625\u062c\u062a\u0645\u0627\u0639\u064a</td>\n      <td colspan=\"2\">{{frappe.session.user}}</td>\n      <td colspan=\"3\">\u0645\u0639\u062f \u0627\u0644\u0635\u0631\u0641</td>\n    </tr>\n    <tr>\n      <td colspan=\"10\">:\u0627\u0644\u0645\u0627\u0644\u064a\u0629</td>\n    </tr>\n    <tr>\n      <td colspan=\"2\"></td>\n      <td colspan=\"8\" height=40px>\ufefb \u0645\u0627\u0646\u0639 \u0645\u0646 \u0635\u0631\u0641 \u0627\u0644\u0645\u0628\u0644\u063a \u0623\u0639\u0644\u0627\u0647 \u0645\u0646 \u062d\u0633\u0627\u0628</td>\n    </tr>\n  </table>\n</div>\n {%- endfor -%}", 
